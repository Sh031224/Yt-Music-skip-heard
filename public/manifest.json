{
  "name": "Yt-Music skip heard",
  "description": "It automatically adjusts the playlist so you don't hear the songs you've heard on YouTube Music within 2 hours.",
  "version": "1.0.0",
  "short_name": "Yt-Music skip heard",
  "permissions": ["storage", "tabs", "activeTab", "https://music.youtube.com/*", "http://music.youtube.com/*"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://music.youtube.com/*"],
      "js": ["yt-music-skip.js"]
    }
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-OPh+8Q09LwKCX25QDFN645SiY6BMNty7lRvOz2uqD5k='; object-src 'self'"
}
